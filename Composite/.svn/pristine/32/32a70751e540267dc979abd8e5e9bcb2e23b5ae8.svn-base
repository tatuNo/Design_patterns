
public class Muistipiiri implements Laiteosa {

	private final int hinta = 10;
	
	@Override
	public int getHinta() {
		return hinta;
	}
	
	@Override
	public void lisaaOsa(Laiteosa osa) {
		throw new RuntimeException
		("Muistipiiriin ei voi lisätä osia");
	}

	@Override
	public void poistaOsa(Laiteosa osa) {
		throw new RuntimeException
		("Muistipiiristä ei voi poistaa osia");
	}
}
